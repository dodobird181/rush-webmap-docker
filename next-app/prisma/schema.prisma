generator client {
  provider = "prisma-client-js"
}

generator markdown {
  provider = "prisma-markdown"
  output   = "./ERD.md"
  title    = "Test Prisma Markdown Database Display"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FeatureCollection {
  id        Int       @id @default(autoincrement())
  name      String    @db.VarChar(255) //The name of the Feature Collection.
  crs       Json?
  features  Json
  createdAt DateTime  @default(now())
}
